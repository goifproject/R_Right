<!-- 회원가입 페이지 -->
<video id="video" preload="auto" autoplay="true" loop="loop" muted="muted" volume="0">
      <source src="/assets/backvideo.mp4">

</video>

<div class="row">
  <div class="col-md-3">
    
  </div>
  <center>
  <div class="col-md-6">
    <div class="login_frame">
    <a href="/"><img src="/assets/logo_hs.png" class="logo_main">
    <img  class="logo-hover-preload" style='display:none'></a><br>
    <h3>:: 회원가입</h3>
    
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>
    
      <div class="field">
        <%= f.label :이메일, :class => "login_label" %><br />
        <%= f.email_field :email, autofocus: true, autocomplete: "email", :class => "login_field" %>
      </div>
    
    <div class="form-group">
       <% f.label :count %>
       <% f.text_field :count, autofocus: true, class: 'form-control' %>
    </div>
    
    <div class="form-group">
       <% f.label :final %>
       <% f.text_field :final, autofocus: true, class: 'form-control' %>
    </div>
    
      <div class="field">
        <%= f.label :비밀번호, :class => "login_label" %>
        <% if @minimum_password_length %>
        <em class="login_label2">(<%= @minimum_password_length %>자리 이상 입력해주세요.)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "new-password", :class => "login_field" %>
      </div>
    
      <div class="field">
        <%= f.label :비밀번호_확인, :class => "login_label" %><br />
        <%= f.password_field :password_confirmation, autocomplete: "new-password", :class => "login_field" %>
      </div>
    <br>
      <div class="actions">
        <%= f.submit "가입하기", :class => "button_login" %>
      </div>
    <% end %>
    
  </div>
  </div>
  </center>
  <div class="col-md-3"></div>
</div>